<% provide(:title, '') %>
<body class="landing">

  <div class="landing__brandbox fade-in">
    <h1 class="landing__brandbox__title">Felicity Ng</h1>
    <p class="landing__brandbox__descriptor">UX. UI. Designer.</p>
  </div>

  <div class="landing__navbox landing__navbox--left in-from-left">
    <div id="aboutButton" class="landing__navbox__link">
      <div class="landing__navbox__link__img" id="leftarrow"></div>
      <h1 class="landing__navbox__link__h1">About</h1>
    </div>
  </div>
  <div class="landing__navbox landing__navbox--right in-from-right">
    <div id="projectsButton" class="landing__navbox__link">
      <h1 class="landing__navbox__link__h1">Projects</h1>
      <div class="landing__navbox__link__img" id="rightarrow"></div>
    </div>
  </div>

  <%= javascript_include_tag 'home', 'data-turbolinks-track': 'reload' %>
</body>
